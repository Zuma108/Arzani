            upgradeBtns.forEach(btn => {
                btn.addEventListener('click', function(e) {
                    const plan = this.getAttribute('href').split('/').pop();
                    window.location.href = `/checkout/${plan}`;
                });
            });
            
            // Pricing toggle functionality
            const sellerPlansBtn = document.getElementById('seller-plans');
            const buyerPlansBtn = document.getElementById('buyer-plans');
            const sellerPricing = document.getElementById('seller-pricing');
            const buyerPricing = document.getElementById('buyer-pricing');
            
            if (sellerPlansBtn && buyerPlansBtn) {
                sellerPlansBtn.addEventListener('click', function() {
                    sellerPlansBtn.classList.add('active');
                    buyerPlansBtn.classList.remove('active');
                    sellerPricing.classList.remove('hidden');
                    buyerPricing.classList.add('hidden');
                });
                
                buyerPlansBtn.addEventListener('click', function() {
                    buyerPlansBtn.classList.add('active');
                    sellerPlansBtn.classList.remove('active');
                    buyerPricing.classList.remove('hidden');
                    sellerPricing.classList.add('hidden');
                });
            }

            // Enhanced FAQ accordion functionality
            document.querySelectorAll('.faq-question').forEach(question => {
                question.addEventListener('click', () => {
                    const faqItem = question.parentElement;
                    const answer = faqItem.querySelector('.faq-answer');
                    const icon = question.querySelector('.faq-icon');
                    const isActive = faqItem.classList.contains('active');
                    
                    // Close all other FAQ items
                    document.querySelectorAll('.faq-item').forEach(item => {
                        if (item !== faqItem) {
                            item.classList.remove('active');
                            const otherAnswer = item.querySelector('.faq-answer');
                            const otherIcon = item.querySelector('.faq-icon');
                            if (otherAnswer) otherAnswer.style.maxHeight = '0px';
                            if (otherIcon) otherIcon.style.transform = 'rotate(0deg)';
                        }
                    });
                    
                    // Toggle current item
                    if (isActive) {
                        faqItem.classList.remove('active');
                        answer.style.maxHeight = '0px';
                        icon.style.transform = 'rotate(0deg)';
                    } else {
                        faqItem.classList.add('active');
                        answer.style.maxHeight = answer.scrollHeight + 'px';
                        icon.style.transform = 'rotate(180deg)';
                    }
                });
            });
        });
    </script>

