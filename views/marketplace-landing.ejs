    <!-- Business Slider JavaScript -->
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            let businesses = [];
            let currentSlide = 0;
            let slidesToShow = 3;
            let autoSlideInterval = null;
            
            // Calculate how many slides to show based on screen size
            function calculateSlidesToShow() {
                const width = window.innerWidth;
                
                // Determine slidesToShow based on screen width
                if (width < 640) {
                    slidesToShow = 1;
                } else if (width < 1024) {
                    slidesToShow = 2;
                } else {
                    slidesToShow = 3;
                }
                
                // Update card widths and flex properties based on slidesToShow
                const cards = document.querySelectorAll('.business-card-white');
                const track = document.getElementById('businessSliderTrack');
                const containerWidth = track ? track.parentNode.clientWidth : 0;
                
                // Calculate card width with margins
                const cardGap = 24; // gap between cards in pixels
                let cardWidth;
                
                if (slidesToShow === 1) {
                    cardWidth = containerWidth - (cardGap * 2) - 30; // Single card, less margin
                } else if (slidesToShow === 2) {
                    cardWidth = (containerWidth / 2) - (cardGap * 1.5); // Two cards
                } else {
                    cardWidth = (containerWidth / 3) - (cardGap * 1.2); // Three cards
                }
                
                // Apply width to cards
                cards.forEach(card => {
                    // Set flex basis and width
                    if (slidesToShow === 1) {
                        card.style.flex = '0 0 calc(100% - 16px)';
                        card.style.maxWidth = `${cardWidth}px`;
                    } else if (slidesToShow === 2) {
                        card.style.flex = '0 0 calc(50% - 16px)';
                        card.style.maxWidth = `${cardWidth}px`;
                    } else {
                        card.style.flex = '0 0 calc(33.333% - 16px)';
                        card.style.maxWidth = `${cardWidth}px`;
                    }
                });
                
                // Adjust current slide if needed to avoid empty space
                if (businesses && businesses.length) {
                    const maxSlide = Math.max(0, Math.ceil(businesses.length / slidesToShow) - 1);
                    if (currentSlide > maxSlide) {
                        currentSlide = maxSlide;
                        updateSlider();
                    }
                }
            }

            // Show loading state
            function showLoadingState() {
                const loadingState = document.querySelector('.loading-state-white');
                if (loadingState) {
                    loadingState.style.display = 'flex';
                }
            }

            // Hide loading state
            function hideLoadingState() {
                const loadingState = document.querySelector('.loading-state-white');
                if (loadingState) {
                    loadingState.style.display = 'none';
                }
            }

            // Show error message
            function showErrorMessage() {
                const track = document.getElementById('businessSliderTrack');
                if (track) {
                    track.innerHTML = `
                        <div class="w-full flex items-center justify-center py-16">
                            <div class="text-center">
                                <svg class="w-16 h-16 text-gray-400 mx-auto mb-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4m0 4h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z"/>
                                </svg>
                                <p class="text-gray-600 text-lg">Unable to load featured businesses</p>
                                <p class="text-gray-500 text-sm mt-2">Please try again later</p>
                            </div>
                        </div>
                    `;
                    track.style.opacity = '1';
                }
            }

            // Show no data message
            function showNoDataMessage() {
                const track = document.getElementById('businessSliderTrack');
                if (track) {
                    track.innerHTML = `
                        <div class="w-full flex items-center justify-center py-16">
                            <div class="text-center">
                                <svg class="w-16 h-16 text-gray-400 mx-auto mb-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4"/>
                                </svg>
                                <p class="text-gray-600 text-lg">No featured businesses available</p>
                                <p class="text-gray-500 text-sm mt-2">Check back soon for new listings</p>
                            </div>
                        </div>
                    `;
                    track.style.opacity = '1';
                }
            }

            // Fetch business data from API
            async function fetchBusinessData() {
                showLoadingState();
                try {
                    const response = await fetch('/api/business-preview/featured');
                    if (!response.ok) {
                        throw new Error('Failed to fetch business data');
                    }
                    businesses = await response.json();
                    if (businesses.length > 0) {
                        renderSlider();
                        initializeNavButtons();
                        calculateSlidesToShow();
                        startAutoSlide();
                    } else {
                        showNoDataMessage();
                    }
                } catch (error) {
                    console.error('Error fetching business data:', error);
                    showErrorMessage();
                } finally {
                    hideLoadingState();
                }
            }

            // Format currency values
            function formatCurrency(value) {
                if (value >= 1000000) {
                    return '£' + (value / 1000000).toFixed(1) + 'M';
                } else if (value >= 1000) {
                    return '£' + (value / 1000).toFixed(0) + 'K';
                } else {
                    return '£' + value.toLocaleString();
                }
            }

            // Create business card HTML with white theme styling
            function createBusinessCard(business) {
                return `
                    <div class="business-card-white transition-all duration-300 hover:-translate-y-1 overflow-hidden" onclick="window.location.href='/signup'">
                        <div class="business-card-header-white">
                            <div class="business-card-category-white">
                                ${business.industry || 'Business'}
                            </div>
                            <h3 class="business-card-title-white">
                                ${business.name}
                            </h3>
                            <div class="business-card-location-white">
                                <svg xmlns="http://www.w3.org/2000/svg" class="h-4 w-4" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z" />
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z" />
                                </svg>
                                ${business.location || 'UK'}
                            </div>
                        </div>
                        <div class="business-card-body-white">
                            <div class="business-metrics-white">
                                <div class="metric-item-white" data-type="price">
                                    <div class="metric-label-white">ASKING PRICE</div>
                                    <div class="metric-value-white">${formatCurrency(business.price)}</div>
                                </div>
                                <div class="metric-item-white" data-type="revenue">
                                    <div class="metric-label-white">REVENUE</div>
                                    <div class="metric-value-white">${formatCurrency(business.revenue)}</div>
                                </div>
                                <div class="metric-item-white" data-type="profit">
                                    <div class="metric-label-white">PROFIT</div>
                                    <div class="metric-value-white">${formatCurrency(business.profit)}</div>
                                </div>
                            </div>
                        </div>
                    </div>
                `;
            }

            // Render the slider
            function renderSlider() {
                const track = document.getElementById('businessSliderTrack');
                if (!track || businesses.length === 0) return;

                // Create HTML for all business cards
                track.innerHTML = businesses.map(createBusinessCard).join('');
                
                // Add a slight delay before showing the track with a fade-in effect
                setTimeout(() => {
                    track.style.opacity = '1';
                    
                    // Add animation to cards
                    const cards = track.querySelectorAll('.business-card-white');
                    cards.forEach((card, index) => {
                        card.style.opacity = '0';
                        card.style.transform = 'translateY(20px)';
                        
                        setTimeout(() => {
                            card.style.opacity = '1';
                            card.style.transform = 'translateY(0)';
                        }, 100 + (index * 100)); // Stagger the animations
                    });
                    
                    updateSlider();
                    createDots();
                }, 300);
            }

            // Update slider position
            function updateSlider() {
                const track = document.getElementById('businessSliderTrack');
                if (!track || businesses.length === 0) return;
                
                // Get first card to calculate measurements
                const firstCard = track.children[0];
                if (!firstCard) return;
                
                // Calculate card and container dimensions
                const cardRect = firstCard.getBoundingClientRect();
                const cardWidth = cardRect.width;
                const cardGap = 24; // Gap between cards in pixels
                
                // Calculate total width of a slide (card + gap)
                const slideWidth = cardWidth + cardGap;
                
                // Calculate how many full slides we have
                const totalSlides = Math.ceil(businesses.length / slidesToShow);
                
                // Ensure current slide is within bounds
                currentSlide = Math.max(0, Math.min(currentSlide, totalSlides - 1));
                
                // Calculate transition distance
                let transitionDistance;
                
                if (slidesToShow === 1) {
                    // For 1 card per slide, move by the full card width + gap
                    transitionDistance = slideWidth * currentSlide;
                } else {
                    // For multiple cards per slide, adjust the movement
                    // Use the number of full slides passed
                    transitionDistance = slideWidth * currentSlide * slidesToShow;
                    
                    // Make sure we don't go beyond the available slides
                    const remainingCards = businesses.length - (currentSlide * slidesToShow);
                    if (remainingCards < slidesToShow) {
                        // Adjust for last partial slide
                        transitionDistance = slideWidth * (businesses.length - slidesToShow);
                    }
                }
                
                // Apply the transform
                track.style.transform = `translateX(-${transitionDistance}px)`;
                
                // Update navigation elements
                updateDots();
                updateNavButtons();
            }

            // Create navigation dots
            function createDots() {
                const dotsContainer = document.getElementById('sliderDots');
                if (!dotsContainer || businesses.length === 0) return;

                // Calculate how many dots we need based on number of slides
                const totalSlides = Math.ceil(businesses.length / slidesToShow);
                
                // Generate HTML for dots
                const dots = Array.from({ length: totalSlides }, (_, i) => 
                    `<button class="slider-dot w-3 h-3 rounded-full transition-all duration-300 ${i === currentSlide ? 'bg-primary active' : 'bg-gray-300 hover:bg-gray-400'}" 
                             data-slide="${i}" 
                             aria-label="Go to slide ${i + 1}">
                     </button>`
                ).join('');

                dotsContainer.innerHTML = dots;

                // Add click handlers to dots
                dotsContainer.querySelectorAll('.slider-dot').forEach((dot, index) => {
                    dot.addEventListener('click', () => {
                        currentSlide = index;
                        updateSlider();
                        stopAutoSlide();
                        startAutoSlide();
                    });
                });
            }

            // Update dots appearance
            function updateDots() {
                const dots = document.querySelectorAll('.slider-dot');
                const totalSlides = Math.ceil(businesses.length / slidesToShow);
                
                // If there are no dots or no businesses, don't proceed
                if (!dots.length || businesses.length === 0) return;
                
                // Calculate active dot index
                const activeDotIndex = Math.min(currentSlide, totalSlides - 1);
                
                dots.forEach((dot, index) => {
                    // Remove all classes first
                    dot.classList.remove('bg-primary', 'bg-gray-300', 'hover:bg-gray-400', 'active');
                    
                    // Add appropriate classes
                    if (index === activeDotIndex) {
                        dot.classList.add('bg-primary', 'active');
                    } else {
                        dot.classList.add('bg-gray-300', 'hover:bg-gray-400');
                    }
                });
            }

            // Update navigation buttons
            function updateNavButtons() {
                const leftBtn = document.getElementById('slideLeft');
                const rightBtn = document.getElementById('slideRight');
                
                // If buttons don't exist or no businesses, don't proceed
                if (!leftBtn || !rightBtn || businesses.length === 0) return;
                
                const totalSlides = Math.ceil(businesses.length / slidesToShow);
                const maxSlideIndex = Math.max(0, totalSlides - 1);
                
                // Update left button state
                if (currentSlide <= 0) {
                    leftBtn.disabled = true;
                    leftBtn.classList.add('opacity-40', 'cursor-not-allowed');
                    leftBtn.classList.remove('hover:bg-primary', 'hover:text-white', 'hover:scale-110');
                } else {
                    leftBtn.disabled = false;
                    leftBtn.classList.remove('opacity-40', 'cursor-not-allowed');
                    leftBtn.classList.add('hover:bg-primary', 'hover:text-white', 'hover:scale-110');
                }
                
                // Update right button state
                if (currentSlide >= maxSlideIndex) {
                    rightBtn.disabled = true;
                    rightBtn.classList.add('opacity-40', 'cursor-not-allowed');
                    rightBtn.classList.remove('hover:bg-primary', 'hover:text-white', 'hover:scale-110');
                } else {
                    rightBtn.disabled = false;
                    rightBtn.classList.remove('opacity-40', 'cursor-not-allowed');
                    rightBtn.classList.add('hover:bg-primary', 'hover:text-white', 'hover:scale-110');
                }
            }

            // Navigate to previous slide
            function prevSlide() {
                if (currentSlide > 0) {
                    currentSlide--;
                    updateSlider();
                }
            }

            // Navigate to next slide
            function nextSlide() {
                const totalSlides = Math.ceil(businesses.length / slidesToShow);
                if (currentSlide < totalSlides - 1) {
                    currentSlide++;
                    updateSlider();
                }
            }

            // Auto slide functionality
            function startAutoSlide() {
                stopAutoSlide();
                autoSlideInterval = setInterval(() => {
                    const totalSlides = Math.ceil(businesses.length / slidesToShow);
                    
                    // Move to next slide or loop back to first slide
                    if (currentSlide >= totalSlides - 1) {
                        currentSlide = 0;
                    } else {
                        currentSlide++;
                    }
                    
                    updateSlider();
                }, 5000);
            }

            function stopAutoSlide() {
                if (autoSlideInterval) {
                    clearInterval(autoSlideInterval);
                    autoSlideInterval = null;
                }
            }

            // Handle window resize
            function handleResize() {
                calculateSlidesToShow();
                updateSlider();
            }

            // Initialize navigation buttons
            function initializeNavButtons() {
                const slideLeftBtn = document.getElementById('slideLeft');
                const slideRightBtn = document.getElementById('slideRight');
                
                if (slideLeftBtn) {
                    slideLeftBtn.addEventListener('click', () => {
                        prevSlide();
                        stopAutoSlide();
                        startAutoSlide();
                    });
                }
                
                if (slideRightBtn) {
                    slideRightBtn.addEventListener('click', () => {
                        nextSlide();
                        stopAutoSlide();
                        startAutoSlide();
                    });
                }
            }

            // Initialize slider
            fetchBusinessData();
            
            // Add window resize listener with debounce for better performance
            let resizeTimeout;
            window.addEventListener('resize', () => {
                clearTimeout(resizeTimeout);
                resizeTimeout = setTimeout(handleResize, 100);
            });

            // Cleanup on page unload
            window.addEventListener('beforeunload', () => {
                stopAutoSlide();
            });
            
            // Business Intelligence Metrics Dashboard Animations
            function initMetricsDashboard() {
                // Animate progress bars
                const progressBars = document.querySelectorAll('.metric-progress-bar');
                progressBars.forEach(bar => {
                    const targetWidth = bar.getAttribute('data-target-width');
                    setTimeout(() => {
                        bar.style.width = targetWidth;
                    }, 500);
                });
                
                // Animate counter values
                const counterElements = document.querySelectorAll('.data-value[data-count-value]');
                counterElements.forEach(counter => {
                    const targetValue = parseFloat(counter.getAttribute('data-count-value'));
                    const suffix = counter.getAttribute('data-count-suffix') || '';
                    const duration = 2000; // 2 seconds
                    const frameRate = 30; // frames per second
                    const totalFrames = duration / (1000 / frameRate);
                    const valueIncrement = targetValue / totalFrames;
                    let currentValue = 0;
                    let currentFrame = 0;
                    
                    const animate = () => {
                        currentFrame++;
                        currentValue += valueIncrement;
                        
                        if (currentFrame <= totalFrames) {
                            let displayValue;
                            if (targetValue >= 100) {
                                displayValue = Math.round(currentValue);
                            } else {
                                displayValue = currentValue.toFixed(1);
                            }
                            counter.textContent = displayValue + suffix;
                            requestAnimationFrame(animate);
                        } else {
                            counter.textContent = targetValue + suffix;
                        }
                    };
                    
                    requestAnimationFrame(animate);
                });
                
                // Initialize tooltips
                const tooltipTriggers = document.querySelectorAll('.metric-tooltip-trigger');
                tooltipTriggers.forEach(trigger => {
                    trigger.addEventListener('mouseenter', () => {
                        const tooltip = trigger.parentElement.querySelector('.metric-tooltip');
                        if (tooltip) {
                            tooltip.classList.remove('hidden');
                        }
                    });
                    
                    trigger.addEventListener('mouseleave', () => {
                        const tooltip = trigger.parentElement.querySelector('.metric-tooltip');
                        if (tooltip) {
                            tooltip.classList.add('hidden');
                        }
                    });
                });
                
                // Time filter functionality
                const timeFilters = document.querySelectorAll('.time-filter button');
                timeFilters.forEach(filter => {
                    filter.addEventListener('click', () => {
                        // Remove active class from all filters
                        timeFilters.forEach(f => f.classList.remove('active-filter'));
                        // Add active class to clicked filter
                        filter.classList.add('active-filter');
                    });
                });
            }
            
            // Initialize metrics dashboard when page loads
            setTimeout(initMetricsDashboard, 1000);
        });
    </script>
</body>
</html>
