    </script>    <!-- Business Preview Slider JavaScript -->
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            let currentSlide = 0;
            let businesses = [];
            let slideInterval;
            let isHovered = false;
            let slidesToShow = 1;
            let cardWidth = 0;
            let cardGap = 20; // Gap between cards in pixels

            // Show loading state
            const showLoadingState = () => {
                const loadingState = document.querySelector('.loading-state');
                if (loadingState) loadingState.classList.remove('hidden');
                
                const sliderTrack = document.getElementById('businessSliderTrack');
                if (sliderTrack) sliderTrack.classList.add('opacity-0');
            };
            
            // Hide loading state
            const hideLoadingState = () => {
                const loadingState = document.querySelector('.loading-state');
                if (loadingState) loadingState.classList.add('hidden');
                
                const sliderTrack = document.getElementById('businessSliderTrack');
                if (sliderTrack) sliderTrack.classList.remove('opacity-0');
            };              // Calculate slides to show based on screen size
            function calculateSlidesToShow() {
                const prevSlidesToShow = slidesToShow;
                
                // Increase number of visible slides based on screen size
                if (window.innerWidth >= 1600) {
                    slidesToShow = 5; // Show 5 on very large screens
                } else if (window.innerWidth >= 1400) {
                    slidesToShow = 4; // Show 4 on large screens
                } else if (window.innerWidth >= 1280) {
                    slidesToShow = 3; // Show 3 on medium screens
                } else if (window.innerWidth >= 768) {
                    slidesToShow = 2; // Show 2 on tablets
                } else {
                    slidesToShow = 1; // Show 1 on mobile
                }
                
                // Get actual card width for precise calculations (including gap)
                const track = document.getElementById('businessSliderTrack');
                if (track && track.children.length > 0) {
                    const firstCard = track.children[0];
                    const computedStyle = window.getComputedStyle(track);
                    const gapSize = parseInt(computedStyle.gap) || cardGap;
                    cardWidth = firstCard.offsetWidth + gapSize;
                    
                    // Adjust the track width to accommodate all business cards
                    if (businesses.length > 0) {
                        // Make track wide enough for all cards plus gaps
                        const totalWidth = (businesses.length * firstCard.offsetWidth) + ((businesses.length - 1) * gapSize);
                        track.style.width = `${totalWidth}px`;
                    }
                }
                
                // If slidesToShow changed, we need to recalculate
                if (prevSlidesToShow !== slidesToShow) {
                    renderDots();
                    goToSlide(Math.min(currentSlide, Math.max(0, businesses.length - slidesToShow)));
                }
                
                // Make sure we show all businesses, even on resize
                if (businesses.length > 0) {
                    // Force rerender of slider if we have no children yet
                    if (track && track.children.length < businesses.length) {
                        renderSlider();
                    }
                }
            }

            // Fetch business data from API
            async function fetchBusinessData() {
                showLoadingState();
                try {
                    const response = await fetch('/api/business-preview/featured');
                    if (!response.ok) {
                        throw new Error('Failed to fetch business data');
                    }
                    businesses = await response.json();
                    if (businesses.length > 0) {
                        renderSlider();
                        initializeNavButtons();
                        calculateSlidesToShow();
                        startAutoSlide();
                    } else {
                        showNoDataMessage();
                    }
                } catch (error) {
                    console.error('Error fetching business data:', error);
                    showErrorMessage();
                } finally {
                    hideLoadingState();
                }
            }

            // Format currency values
            function formatCurrency(value) {
                if (value >= 1000000) {
                    return '£' + (value / 1000000).toFixed(1) + 'M';
                } else if (value >= 1000) {
                    return '£' + (value / 1000).toFixed(0) + 'K';
                } else {
                    return '£' + value.toLocaleString();
                }
            }            // Create business card HTML with signup redirect
            function createBusinessCard(business) {
                return `
                    <div class="business-card cursor-pointer" onclick="window.location.href='/signup'">
                        <div class="business-card-header">
                            <div class="business-card-overlay">
                                <span class="business-card-category">${business.industry || 'Business'}</span>
                                <h3 class="business-card-title">${business.name}</h3>
                                <div class="business-card-location">
                                    <svg xmlns="http://www.w3.org/2000/svg" class="h-4 w-4" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z" />
                                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z" />
                                    </svg>
                                    ${business.location || 'UK'}
                                </div>
                            </div>
                        </div>
                        <div class="business-card-body">
                            <div class="business-metrics">
                                <div class="metric-item">
                                    <div class="metric-label">Asking Price</div>
                                    <div class="metric-value">${formatCurrency(business.price)}</div>
                                </div>
                                <div class="metric-item">
                                    <div class="metric-label">Annual Revenue</div>
                                    <div class="metric-value">${formatCurrency(business.revenue)}</div>
                                </div>
                                <div class="metric-item">
                                    <div class="metric-label">Annual Profit</div>
                                    <div class="metric-value">${formatCurrency(business.profit)}</div>
                                </div>
                            </div>
                        </div>
                    </div>
                `;
            }

            // Render the slider
            function renderSlider() {
                const track = document.getElementById('businessSliderTrack');
                
                if (!track) return;

                // Clear existing content
                track.innerHTML = '';

                // Add business cards
                businesses.forEach((business, index) => {
                    track.innerHTML += createBusinessCard(business);
                });
                
                // After cards are rendered, calculate slides to show
                calculateSlidesToShow();
                
                // Create dots now that we know how many slides we have
                renderDots();

                // Add hover listeners to pause auto-slide
                const sliderContainer = document.querySelector('.business-slider-container');
                if (sliderContainer) {
                    sliderContainer.addEventListener('mouseenter', () => {
                        isHovered = true;
                        stopAutoSlide();
                    });
                    
                    sliderContainer.addEventListener('mouseleave', () => {
                        isHovered = false;
                        startAutoSlide();
                    });
                }
                
                // Reset to first slide after render
                currentSlide = 0;
                goToSlide(0);
            }
            
            // Render the navigation dots
            function renderDots() {
                const dotsContainer = document.getElementById('sliderDots');
                if (!dotsContainer) return;
                
                // Clear existing dots
                dotsContainer.innerHTML = '';
                
                // Calculate max number of possible slides
                const maxSlides = Math.max(1, businesses.length - slidesToShow + 1);
                
                // Add navigation dots only for the number of possible slide positions
                for (let i = 0; i < maxSlides; i++) {
                    const dot = document.createElement('button');
                    dot.className = `slider-dot ${i === currentSlide ? 'active' : ''}`;
                    dot.setAttribute('aria-label', `Go to slide ${i + 1}`);
                    dot.addEventListener('click', () => goToSlide(i));
                    dotsContainer.appendChild(dot);
                }
            }            // Go to specific slide with improved calculation
            function goToSlide(slideIndex) {
                const track = document.getElementById('businessSliderTrack');
                const dots = document.querySelectorAll('.slider-dot');
                
                if (!track || track.children.length === 0) return;
                
                // Calculate max slides to prevent going beyond available content
                const maxSlides = Math.max(1, businesses.length - slidesToShow + 1);
                
                // Ensure slideIndex is within bounds
                currentSlide = Math.max(0, Math.min(slideIndex, maxSlides - 1));
                
                // Get the container width and card width for precise pixel calculations
                const container = document.querySelector('.business-slider-container');
                const containerWidth = container ? container.clientWidth : 0;
                const cardElement = track.children[0];
                const cardElementWidth = cardElement ? cardElement.offsetWidth : 0;
                const computedStyle = window.getComputedStyle(track);
                const gapSize = parseInt(computedStyle.gap) || cardGap;
                
                // Calculate the total width of a card including the gap
                const totalCardWidth = cardElementWidth + gapSize;
                
                // Fix for the gap at the end - calculate exact pixel amount to move
                // This ensures we don't move too far on the last slide
                let translateXPixels;
                
                if (currentSlide === maxSlides - 1 && businesses.length > slidesToShow) {
                    // If we're on the last slide, calculate the exact amount to prevent the gap
                    const remainingCards = businesses.length - slidesToShow;
                    translateXPixels = remainingCards * totalCardWidth;
                } else {
                    // Normal slide calculation
                    translateXPixels = currentSlide * totalCardWidth;
                }
                
                // Apply the transform
                track.style.transform = `translateX(-${translateXPixels}px)`;
                
                // Update dots
                dots.forEach((dot, index) => {
                    dot.classList.toggle('active', index === currentSlide);
                });
            }

            // Auto-slide functionality
            function startAutoSlide() {
                if (slideInterval) clearInterval(slideInterval);
                const maxSlides = Math.max(1, businesses.length - slidesToShow + 1);
                
                if (maxSlides > 1) {
                    slideInterval = setInterval(() => {
                        if (!isHovered) {
                            nextSlide();
                        }
                    }, 5000); // 5 seconds
                }
            }

            function stopAutoSlide() {
                if (slideInterval) {
                    clearInterval(slideInterval);
                    slideInterval = null;
                }
            }

            function nextSlide() {
                const maxSlides = Math.max(1, businesses.length - slidesToShow + 1);
                currentSlide = (currentSlide + 1) % maxSlides;
                goToSlide(currentSlide);
            }

            function prevSlide() {
                const maxSlides = Math.max(1, businesses.length - slidesToShow + 1);
                currentSlide = (currentSlide - 1 + maxSlides) % maxSlides;
                goToSlide(currentSlide);
            }

            // Handle window resize
            function handleResize() {
                calculateSlidesToShow();
            }

            // Show no data message
            function showNoDataMessage() {
                const track = document.getElementById('businessSliderTrack');
                if (track) {
                    track.innerHTML = `
                        <div class="business-slide">
                            <div class="business-card" style="text-align: center; padding: 3rem;">
                                <p class="text-gray-600">No featured businesses available.</p>
                                <p class="text-gray-500 text-sm mt-2">Check back soon for new listings!</p>
                            </div>
                        </div>
                    `;
                }
            }
            
            // Initialize navigation buttons
            function initializeNavButtons() {
                const slideLeftBtn = document.getElementById('slideLeft');
                const slideRightBtn = document.getElementById('slideRight');
                
                if (slideLeftBtn) {
                    slideLeftBtn.addEventListener('click', () => {
                        prevSlide();
                        stopAutoSlide();
                        startAutoSlide();
                    });
                }
                
                if (slideRightBtn) {
                    slideRightBtn.addEventListener('click', () => {
                        nextSlide();
                        stopAutoSlide();
                        startAutoSlide();
                    });
                }
            }

            // Initialize slider
            fetchBusinessData();

            // Add window resize listener with debounce for better performance
            let resizeTimeout;
            window.addEventListener('resize', () => {
                clearTimeout(resizeTimeout);
                resizeTimeout = setTimeout(handleResize, 100);
            });

            // Cleanup on page unload
            window.addEventListener('beforeunload', () => {
                stopAutoSlide();
            });
        });
    </script>
