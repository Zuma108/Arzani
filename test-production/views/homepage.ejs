                        <script>
                            document.addEventListener('DOMContentLoaded', function() {
                                // Enhanced animated counters with easing function
                                const countElements = document.querySelectorAll('.data-value');
                                
                                function animateCounters() {
                                    countElements.forEach(element => {
                                        const targetValue = parseFloat(element.getAttribute('data-count-value'));
                                        const suffix = element.getAttribute('data-count-suffix') || '';
                                        let startValue = 0;
                                        let currentValue = 0;
                                        const duration = 2500; // 2.5 seconds
                                        const framesPerSecond = 60;
                                        const totalFrames = duration / (1000 / framesPerSecond);
                                        let frame = 0;
                                        
                                        // Easing function for more natural animation
                                        function easeOutQuad(t) {
                                            return t * (2 - t);
                                        }
                                        
                                        const counter = setInterval(() => {
                                            frame++;
                                            // Apply easing for smoother animation
                                            const progress = easeOutQuad(frame / totalFrames);
                                            currentValue = startValue + (targetValue - startValue) * progress;
                                            
                                            if (frame >= totalFrames) {
                                                currentValue = targetValue;
                                                clearInterval(counter);
                                                
                                                // Add a pulse effect when counter completes
                                                element.classList.add('pulse-once');
                                                setTimeout(() => {
                                                    element.classList.remove('pulse-once');
                                                }, 1000);
                                            }
                                            
                                            // Format with appropriate decimal places
                                            const formattedValue = targetValue >= 10 
                                                ? currentValue.toFixed(1) 
                                                : currentValue.toFixed(2);
                                                
                                            element.textContent = formattedValue + suffix;
                                        }, 1000 / framesPerSecond);
                                    });
                                }
                                
                                // Initialize tooltips for metric annotations
