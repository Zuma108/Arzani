                    const originalPosition = ctaWrapper.offsetTop;
                    const aiSection = document.querySelector('.ai-hero-section');
                    const featureSection = document.querySelector('#features-section');
                    
                    // Track when user has seen the CTA
                    let hasInteractedWithCTA = false;
                    
                    // Add gentle pulse animation to make the button more noticeable
                    if (stickyCTA) {
                        setTimeout(() => {
                            stickyCTA.classList.add('animate-pulse-gentle');
