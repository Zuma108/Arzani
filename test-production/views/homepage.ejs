                    // Show countdown after a delay for better engagement
                    if (ctaCountdown) {
                        setTimeout(() => {
                            ctaCountdown.classList.remove('opacity-0');
                        }, 3000);
                    }
                    
                    // Improved scroll tracking with smooth transitions and better boundaries
                    window.addEventListener('scroll', function() {
                        const scrollPosition = window.pageYOffset;
                        const aiSectionBottom = aiSection.offsetTop + aiSection.offsetHeight;
                        const viewportHeight = window.innerHeight;
                        
                        // Calculate if we're in the valid scroll range: after seeing enough of the AI section
                        // but before reaching the features section
                        const scrolledPastContent = scrollPosition > (originalPosition + 250);
                        const notYetAtFeatures = featureSection ? scrollPosition < (featureSection.offsetTop - 100) : true;
                        const inViewThreshold = scrollPosition > (aiSection.offsetTop + 300);
                        
                        if (scrolledPastContent && notYetAtFeatures && inViewThreshold) {
                            // Enhanced sticky behavior with improved animation
                            stickyCTA.classList.add('fixed', 'bottom-10', 'right-8', 'z-50');
                            stickyCTA.classList.add('shadow-lg', 'scale-105');
                            
                            // Add subtle pulse effect for visibility
                            if (!hasInteractedWithCTA) {
                                stickyCTA.classList.add('shadow-[0_6px_16px_rgba(6,79,163,0.35)]');
                                ctaCountdown.classList.remove('opacity-0');
                            }
                        } else {
                            // Return to normal position with smooth transition
                            stickyCTA.classList.remove('fixed', 'bottom-10', 'right-8', 'z-50');
                            stickyCTA.classList.remove('shadow-lg', 'scale-105', 'shadow-[0_6px_16px_rgba(6,79,163,0.35)]');
                            ctaCountdown.classList.add('opacity-0');
                        }
                    });
                    
                    // Track CTA interaction to stop animations once user has engaged
                    stickyCTA.addEventListener('click', function() {
                        hasInteractedWithCTA = true;
                        stickyCTA.classList.remove('animate-pulse-gentle');
