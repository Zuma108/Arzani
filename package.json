{
  "name": "arzani-marketplace",
  "version": "1.0.0",
  "description": "Arzani Business Marketplace Application",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node scripts/start-with-agents.js",
    "start:basic": "node server.js",
    "start:agents-only": "concurrently --kill-others-on-fail \"node services/orchestrator/index.js\" \"node services/broker/index.js\" \"node services/legal/index.js\" \"node services/finance/index.js\"",
    "start:hybrid": "node start-hybrid-system.js",
    "start:mcp": "node scripts/start-mcp-servers.js",
    "test:hybrid": "node scripts/test-hybrid-system.js",
    "dev": "nodemon server.js --inspect",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --passWithNoTests",
    "test:arzani-ai": "cd arzani-ai && node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:oauth": "node test-oauth-flow.js",
    "test:google-oauth-fix": "node test-google-oauth-fix.js",
    "build": "npm run build:css",
    "build:css": "tailwindcss -i ./src/tailwind.css -o ./public/css/tailwind.min.css --minify",
    "build:marketplace-css": "tailwindcss -i ./marketplace-landing-page/tailwind.css -o ./marketplace-landing-page/style.css --minify",
    "analytics:report": "node scripts/generate-analytics-report.js",
    "update-browserslist": "npx update-browserslist-db@latest",
    "test:watch": "jest --watch",
    "lint": "echo 'Linting not configured yet'",
    "migrate": "node scripts/run-migrations.js",
    "tunnel": "ngrok http 5000 --config=ngrok.config.js",
    "postinstall": "node scripts/ensure-ai-assets.js",
    "prepare-deploy": "node scripts/clean-for-deploy.js",
    "add-market-trends-index": "node migrations/add-unique-index-to-market-trends-mv.js",
    "setup-ai-assistant": "node migrations/create_assistant_interactions.js",
    "init-ai": "node scripts/init-ai-assistant.js",
    "fix-ai-tables": "node scripts/fix-ai-tables.js",
    "watch:css": "tailwindcss -i ./src/tailwind.css -o ./public/css/tailwind.min.css --watch",
    "install-blog-deps": "node scripts/install-blog-deps.js",
    "dev:multiprocess": "concurrently \"npm run dev\" \"node services/orchestrator/index.js\" \"node services/broker/index.js\" \"node services/legal/index.js\" \"node services/finance/index.js\"",
    "setup:a2a": "node scripts/a2a-setup.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.777.0",
    "@aws-sdk/s3-request-presigner": "^3.758.0",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@pinecone-database/pinecone": "^6.1.1",
    "@sendgrid/mail": "^8.1.4",
    "@tailwindcss/typography": "^0.5.16",
    "ajv": "^8.17.1",
    "apple-signin-auth": "^1.7.6",
    "axios": "^1.8.2",
    "bcrypt": "^5.x",
    "better-sqlite3": "^11.5.0",
    "body-parser": "^1.20.3",
    "canvas": "^3.1.0",
    "chart.js": "^3.9.1",
    "chartjs-node-canvas": "^4.1.6",
    "compression": "^1.8.0",
    "concurrently": "^9.1.0",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "google-auth-library": "^8.9.0",
    "googleapis": "^144.0.0",
    "helmet": "^8.1.0",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "openai": "^4.104.0",
    "pdfkit": "^0.16.0",
    "pg": "^8.x",
    "puppeteer": "^24.4.0",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "stripe": "^12.18.0",
    "uuid": "^9.0.1",
    "ws": "^8.17.1",
    "xmlbuilder2": "^3.1.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "jest": "^29.7.0",
    "ngrok": "^5.0.0-beta.2",
    "nodemon": "^3.1.9",
    "tailwindcss": "^3.4.17"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
